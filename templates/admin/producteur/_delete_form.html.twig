<form method="post" action="{{ path('producteur_delete', {'id': producteur.id}) }}" onsubmit="return confirm('Es tu sure de vouloir supprimer ce producteur ? ');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ producteur.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
